{{!-- layouts/base.hbs --}}

<!doctype html>
<html lang="en">
	<head>
		<title>
		{{#if title}}
			{{title}}
		{{else}}
			Base Page Title
		{{/if}}
		</title>
	
		{{#> head-block}}
		{{> includes/head }}
		{{/head-block}}


	</head>
	
	<body class="landing-body">

	{{#> landing }}
		{{> content/cont-landing }}
	{{/landing}}
   
	{{#> scripts-block}}
		{{> includes/scripts }}
	{{/scripts-block}}


	</body>
</html>